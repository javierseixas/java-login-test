         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>TestNoAnn</servlet-name>
        <servlet-class>javierseixas.schibsted.TestNoAnn</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestNoAnn</servlet-name>
